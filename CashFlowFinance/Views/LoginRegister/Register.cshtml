<body>

    <div class="account-pages"></div>
    <div class="clearfix"></div>
    <div class="wrapper-page">
        <div class="card-box">
            <div class="panel-heading">
                <h3 class="text-center"> Inscribase en <strong class="text-custom">CashFlow</strong> </h3>
            </div>

            <div class="panel-body">
                <form class="form-horizontal m-t-20" method="post">
                    @Html.HiddenFor(x => x.CuentaId)

                    <div class="form-group ">
                        <div class="col-xs-12">
                            @Html.TextBoxFor(x => x.Username, new { @class = "form-control", @type = "text", @placeholder = "Usuario", @required = "Complete" })
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-xs-12">
                            @Html.TextBoxFor(x => x.Contraseña, new { @class = "form-control", @type = "password", @placeholder = "Contraseña", @required = "Complete" })
                        </div>
                    </div>

                    <div class="form-group ">
                        <div class="col-xs-12">
                            @Html.TextBoxFor(x => x.Correo, new { @class = "form-control", @type = "text", @placeholder = "Correo", @required = "Complete" })
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-xs-12">
                            @Html.TextBoxFor(x => x.Telefono, new { @class = "form-control", @type = "text", @placeholder = "Telefono", @required = "Complete" })
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-xs-12">
                            <div class="checkbox checkbox-primary">
                                <input id="checkbox-signup" type="checkbox" checked="checked">
                                <label for="checkbox-signup">I accept <a href="#">Terms and Conditions</a></label>
                            </div>
                        </div>
                    </div>

                    <div class="form-group text-center m-t-40">
                        <div class="col-xs-12">
                            <button class="btn btn-pink btn-block text-uppercase waves-effect waves-light" type="submit">
                                Continuar
                            </button>
                        </div>
                    </div>

                </form>

            </div>
        </div>

        <div class="row">
            <div class="col-sm-12 text-center">
                <p>
                    Ya tiene una cuenta?<a href="page-login.html" class="text-primary m-l-5"><b>Ingresar</b></a>
                </p>
            </div>
        </div>

    </div>

    <script>
			var resizefunc = [];
    </script>

    <!-- jQuery  -->
    <script src="@Url.Content("~/Content/js/jquery.min.js")"></script>
