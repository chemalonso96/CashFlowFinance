</head>

<body>
    <div class="account-pages"></div>
    <div class="clearfix"></div>
    <div class="wrapper-page">
        <div class="card-box">
            <div class="panel-heading">
                <h1 class="text-center"> <strong class="text-custom">CASH FLOW</strong> </h1>
            </div>


            <div class="panel-body">
                <form class="form-horizontal m-t-20" method="post" >

                    <div class="form-group ">
                        <div class="col-xs-12">
                            <!--INPUT-->
                            @Html.ValidationMessageFor(x=>x.Username)
                            @Html.TextBoxFor(x => x.Username, new { @class = "form-control", @type = "text", @placeholder = "Username" })
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-xs-12">
                            <!--INPUT-->
                            @Html.ValidationMessageFor(x => x.Password)
                            @Html.TextBoxFor(x => x.Password, new { @class = "form-control", @type = "password", @placeholder = "Password" })
                        </div>
                    </div>

                    <div class="form-group ">
                        <div class="col-xs-12">
                            <div class="checkbox checkbox-primary">
                                <input id="checkbox-signup" type="checkbox">
                                <label for="checkbox-signup">
                                    Recordar
                                </label>
                            </div>

                        </div>
                    </div>

                    <div class="form-group text-center m-t-40">
                        <div class="col-xs-12">
                            <button class="btn btn-pink btn-block text-uppercase waves-effect waves-light" type="submit">Iniciar Sesion</button>
                        </div>
                    </div>

                    <div class="form-group m-t-30 m-b-0">
                        <div class="col-sm-12">
                            <a class="text-dark">
                                <i class="fa fa-lock m-r-5"></i> Olvidaste tu contraseña?
                            </a>
                        </div>
                    </div>
                </form>

            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 text-center">
                <p>No tienes una cuenta? <a href="@Url.Action("Register")" class="text-primary m-l-5" ><b>Registrar!</b></a></p>
            </div>
        </div>
    </div>


    <script>
            var resizefunc = [];
    </script>

    <!-- jQuery  -->
    <script src="@Url.Content("~/Content/js/jquery.min.js")"></script>
    <script src="@Url.Content("~/Content/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Content/js/detect.js")"></script>
    <script src="@Url.Content("~/Content/js/fastclick.js")"></script>
    <script src="@Url.Content("~/Content/js/jquery.slimscroll.js")"></script>
    <script src="@Url.Content("~/Content/js/jquery.blockUI.js")"></script>
    <script src="@Url.Content("~/Content/js/waves.js")"></script>
    <script src="@Url.Content("~/Content/js/wow.min.js")"></script>
    <script src="@Url.Content("~/Content/js/jquery.nicescroll.js")"></script>
    <script src="@Url.Content("~/Content/js/jquery.scrollTo.min.js")"></script>


    <script src="@Url.Content("~/Content/js/jquery.core.js")"></script>
    <script src="@Url.Content("~/Content/js/jquery.app.js")"></script>
</body>
</html>

